{
  "name": "dsound",
  "version": "1.0.0",
  "description": "dsound desktop app",
  "main": "main.js",
  "repository": "https://github.com/etaletai13/dsound",
  "author": "Casey Parker <parker.casey@icloud.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "asyncconsole": "*",
    "electron-builder": "~20.28.3",
    "nativefier": "*",
    "prettier-standard": "*"
  },
  "scripts": {
    "build-lin": "yarn build --linux --pd dist/dsound-linux-x64 deb",
    "build-mac": "yarn build --mac --pd dist/dsound-darwin-x64/dsound.app dmg",
    "build-win": "yarn build --win --pd dist/dsound-windows-x64 nsis",
    "postinstall": "yarn prettier *.js *.json --write",
    "prepare": "node main.js && yarn build-mac && yarn build-lin && yarn build-win",
    "test": "ls dist"
  }
}
